{
    "editor.formatOnSave": true,
    "cSpell.words": [
      "commitlint",
      "prettierrc",
      "wangeditor",
      "wangfupeng"
    ],
    "typescript.tsdk": "node_modules/typescript/lib",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
    "eslint.validate": [
      "javascript"
    ],
    "editor.detectIndentation": false,
    "editor.tabSize": 2
}